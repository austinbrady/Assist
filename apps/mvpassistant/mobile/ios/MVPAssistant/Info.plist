<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>CADisableMinimumFrameDurationOnPhone</key>
    <true/>
    <key>CFBundleDevelopmentRegion</key>
    <string>$(DEVELOPMENT_LANGUAGE)</string>
    <key>CFBundleDisplayName</key>
    <string>MVP Assistant</string>
    <key>CFBundleExecutable</key>
    <string>$(EXECUTABLE_NAME)</string>
    <key>CFBundleIdentifier</key>
    <string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
    <key>CFBundleInfoDictionaryVersion</key>
    <string>6.0</string>
    <key>CFBundleName</key>
    <string>$(PRODUCT_NAME)</string>
    <key>CFBundlePackageType</key>
    <string>$(PRODUCT_BUNDLE_PACKAGE_TYPE)</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundleSignature</key>
    <string>????</string>
    <key>CFBundleURLTypes</key>
    <array>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>com.mvpassistant.app</string>
        </array>
      </dict>
    </array>
    <key>CFBundleVersion</key>
    <string>1</string>
    <key>LSRequiresIPhoneOS</key>
    <true/>
    <key>NSAppTransportSecurity</key>
    <dict>
      <key>NSAllowsArbitraryLoads</key>
      <false/>
      <key>NSAllowsLocalNetworking</key>
      <true/>
    </dict>
    <key>NSBluetoothAlwaysUsageDescription</key>
    <string>MVP Assistant uses Bluetooth to connect with nearby devices.</string>
    <key>NSBluetoothPeripheralUsageDescription</key>
    <string>MVP Assistant uses Bluetooth to connect with nearby devices.</string>
    <key>NSCalendarsFullAccessUsageDescription</key>
    <string>Allow MVP Assistant to access your calendar.</string>
    <key>NSCalendarsUsageDescription</key>
    <string>Allow MVP Assistant to access your calendar.</string>
    <key>NSCameraUsageDescription</key>
    <string>Allow MVP Assistant to access your camera.</string>
    <key>NSContactsUsageDescription</key>
    <string>Allow MVP Assistant to access your contacts.</string>
    <key>NSHealthShareUsageDescription</key>
    <string>MVP Assistant can access health data to provide personalized wellness insights.</string>
    <key>NSHealthUpdateUsageDescription</key>
    <string>MVP Assistant can update health data to track your wellness goals.</string>
    <key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
    <string>Allow MVP Assistant to use your location.</string>
    <key>NSLocationAlwaysUsageDescription</key>
    <string>MVP Assistant uses your location to provide context-aware assistance.</string>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>MVP Assistant uses your location to provide context-aware assistance.</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>MVP Assistant needs microphone access for voice commands and dictation.</string>
    <key>NSMotionUsageDescription</key>
    <string>MVP Assistant uses motion sensors to understand your activity and provide better assistance.</string>
    <key>NSPhotoLibraryAddUsageDescription</key>
    <string>Allow $(PRODUCT_NAME) to save photos</string>
    <key>NSPhotoLibraryUsageDescription</key>
    <string>MVP Assistant needs photo library access to help organize and manage your media.</string>
    <key>NSRemindersFullAccessUsageDescription</key>
    <string>Allow $(PRODUCT_NAME) to access your reminders</string>
    <key>NSRemindersUsageDescription</key>
    <string>MVP Assistant needs access to reminders to help manage your tasks.</string>
    <key>UILaunchStoryboardName</key>
    <string>SplashScreen</string>
    <key>UIRequiredDeviceCapabilities</key>
    <array>
      <string>armv7</string>
    </array>
    <key>UIRequiresFullScreen</key>
    <false/>
    <key>UIStatusBarStyle</key>
    <string>UIStatusBarStyleDefault</string>
    <key>UISupportedInterfaceOrientations</key>
    <array>
      <string>UIInterfaceOrientationPortrait</string>
      <string>UIInterfaceOrientationPortraitUpsideDown</string>
    </array>
    <key>UISupportedInterfaceOrientations~ipad</key>
    <array>
      <string>UIInterfaceOrientationPortrait</string>
      <string>UIInterfaceOrientationPortraitUpsideDown</string>
      <string>UIInterfaceOrientationLandscapeLeft</string>
      <string>UIInterfaceOrientationLandscapeRight</string>
    </array>
    <key>UIUserInterfaceStyle</key>
    <string>Automatic</string>
    <key>UIViewControllerBasedStatusBarAppearance</key>
    <false/>
  </dict>
</plist>